{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "outDir": "./data/build",
    "rootDir": "./data/modules",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "include": [
    "data/modules/**/*"
  ]
}
