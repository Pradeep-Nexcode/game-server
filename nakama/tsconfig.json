{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "outDir": "./data/modules/build",
    "rootDir": "./data/modules",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "include": [
    "data/modules/**/*"
  ],
  "exclude": [
    "data/modules/build"
  ]
}
